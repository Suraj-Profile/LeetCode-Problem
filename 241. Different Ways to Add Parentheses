from typing import List

class Solution:
    def diffWaysToCompute(self, expression: str) -> List[int]:
        memo = {}

        def compute(expr):
            if expr in memo:
                return memo[expr]

            results = []

            for i in range(len(expr)):
                if expr[i] in '+-*':
                    left = compute(expr[:i])
                    right = compute(expr[i+1:])

                    for l in left:
                        for r in right:
                            if expr[i] == '+':
                                results.append(l + r)
                            elif expr[i] == '-':
                                results.append(l - r)
                            else:
                                results.append(l * r)

            if not results:
                results.append(int(expr))

            memo[expr] = results
            return results

        return compute(expression)
