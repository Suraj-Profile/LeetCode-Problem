class Solution:
    def getHint(self, secret: str, guess: str) -> str:
        bulls = 0
        cows = 0
        count = [0] * 10

        for i in range(len(secret)):
            if secret[i] == guess[i]:
                bulls += 1
            else:
                s = int(secret[i])
                g = int(guess[i])

                if count[g] > 0:
                    cows += 1
                if count[s] < 0:
                    cows += 1

                count[s] += 1
                count[g] -= 1

        return f"{bulls}A{cows}B"
